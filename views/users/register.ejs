<h2>Register</h2>
<form action="/users/register" method="POST">
    <label>Username</label>
    <input type="text" name="username" value="<%= user.username %>">
    <%- include('_lr_form_fields') %> 
    <button type="submit">Register</button>
</form>
<a href="/">Cancel</a>
<a href="/users/Login">Login</a>